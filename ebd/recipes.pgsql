INSERT INTO tb_step (name, id_recipe) VALUES ('Place corned beef in large pot or Dutch oven and cover with water. Add the spice packet that came with the corned beef. Cover pot and bring to a boil, then reduce to a simmer. Simmer approximately 50 minutes per pound or until tender.', 1);
INSERT INTO tb_step (name, id_recipe) VALUES ('Add whole potatoes and carrots, and cook until the vegetables are almost tender. Add cabbage and cook for 15 more minutes. Remove meat and let rest 15 minutes.', 1);
INSERT INTO tb_step (name, id_recipe) VALUES ('Place vegetables in a bowl and cover. Add as much broth (cooking liquid reserved in the Dutch oven or large pot) as you want. Slice meat across the grain.', 1);
INSERT INTO tb_step (name, id_recipe) VALUES ('In a large bowl, sift together the flour, baking powder, salt and sugar. Make a well in the center and pour in the milk, egg and melted butter; mix until smooth.', 2);
INSERT INTO tb_step (name, id_recipe) VALUES ('Heat a lightly oiled griddle or frying pan over medium-high heat. Pour or scoop the batter onto the griddle, using approximately 1/4 cup for each pancake. Brown on both sides and serve hot.', 2);
INSERT INTO tb_step (name, id_recipe) VALUES ('Place the carrots, potatoes, and onion into the bottom of a slow cooker, pour in the water, and place the brisket on top of the vegetables. Pour the beer over the brisket. Sprinkle on the spices from the packet, cover, and set the cooker on High.', 3);
INSERT INTO tb_step (name, id_recipe) VALUES ('Cook the brisket for about 8 hours. An hour before serving, stir in the cabbage and cook for 1 more hour.', 3);
INSERT INTO tb_step (name, id_recipe) VALUES ('Preheat an oven to 425 degrees F (220 degrees C).', 4);
INSERT INTO tb_step (name, id_recipe) VALUES ('Place the asparagus into a mixing bowl, and drizzle with the olive oil. Toss to coat the spears, then sprinkle with Parmesan cheese, garlic, salt, and pepper. Arrange the asparagus onto a baking sheet in a single layer.', 4);
INSERT INTO tb_step (name, id_recipe) VALUES ('Bake in the preheated oven until just tender, 12 to 15 minutes depending on thickness. Sprinkle with lemon juice just before serving.', 4);
INSERT INTO tb_step (name, id_recipe) VALUES ('Preheat oven to 350 degrees F (175 degrees C).', 5);
INSERT INTO tb_step (name, id_recipe) VALUES ('In a large bowl, combine the beef, egg, onion, milk and bread OR cracker crumbs. Season with salt and pepper to taste and place in a lightly greased 9x5-inch loaf pan, or form into a loaf and place in a lightly greased 9x13-inch baking dish.', 5);
INSERT INTO tb_step (name, id_recipe) VALUES ('In a separate small bowl, combine the brown sugar, mustard and ketchup. Mix well and pour over the meatloaf.', 5);
INSERT INTO tb_step (name, id_recipe) VALUES ('Bake at 350 degrees F (175 degrees C) for 1 hour.', 5);
INSERT INTO tb_step (name, id_recipe) VALUES ('Preheat oven to 350 degrees F (175 degrees C).', 6);
INSERT INTO tb_step (name, id_recipe) VALUES ('Cream together the butter, white sugar, and brown sugar until smooth. Beat in the eggs one at a time, then stir in the vanilla. Dissolve baking soda in hot water. Add to batter along with salt. Stir in flour, chocolate chips, and nuts. Drop by large spoonfuls onto ungreased pans.', 6);
INSERT INTO tb_step (name, id_recipe) VALUES ('Bake for about 10 minutes in the preheated oven, or until edges are nicely browned.', 6);
INSERT INTO tb_step (name, id_recipe) VALUES ('Lightly grease 2 round cake pans with butter.', 7);
INSERT INTO tb_step (name, id_recipe) VALUES ('Roll bread dough out to an 6x18-inch rectangle. Brush with melted butter. Combine brown sugar, walnuts, and cinnamon in a small bowl; sprinkle over butter. Roll dough into a log, starting at the long edge. Moisten edge with water and seal. Cut log into 20 slices; arrange rolls, cut sides down, in prepared cake pans. Cover with a towel and let rise in a warm place until doubled in volume, about 90 minutes.', 7);
INSERT INTO tb_step (name, id_recipe) VALUES ('Preheat oven to 350 degrees F (175 degrees C). Pour heavy cream over dough.', 7);
INSERT INTO tb_step (name, id_recipe) VALUES ('Bake in preheated oven until golden brown, about 25 minutes.', 7);
INSERT INTO tb_step (name, id_recipe) VALUES ('Mix confectioners\' sugar, milk, and vanilla extract in a small bowl; drizzle over warm cinnamon rolls to serve.', 7);
INSERT INTO tb_step (name, id_recipe) VALUES ('In a medium bowl, mash together the avocados, lime juice, and salt. Mix in onion, cilantro, tomatoes, and garlic. Stir in cayenne pepper. Refrigerate 1 hour for best flavor, or serve immediately.', 8);
INSERT INTO tb_step (name, id_recipe) VALUES ('Preheat oven to 350 degrees F (175 degrees C). Grease and flour an 8-inch square pan.', 9);
INSERT INTO tb_step (name, id_recipe) VALUES ('In a large saucepan, melt 1/2 cup butter. Remove from heat, and stir in sugar, eggs, and 1 teaspoon vanilla. Beat in 1/3 cup cocoa, 1/2 cup flour, salt, and baking powder. Spread batter into prepared pan.', 9);
INSERT INTO tb_step (name, id_recipe) VALUES ('Bake in preheated oven for 25 to 30 minutes. Do not overcook.', 9);
INSERT INTO tb_step (name, id_recipe) VALUES ('To Make Frosting: Combine 3 tablespoons softened butter, 3 tablespoons cocoa, honey, 1 teaspoon vanilla extract, and 1 cup confectioners\' sugar. Stir until smooth. Frost brownies while they are still warm.', 9);
INSERT INTO tb_step (name, id_recipe) VALUES ('Place the beef and butter into a slow cooker. Sprinkle the onion soup mix over. Cover, and cook on Low for 8 hours, or High for 4 to 5 hours. Stir once or twice.', 10);
INSERT INTO tb_step (name, id_recipe) VALUES ('Melt butter in a medium saucepan over medium low heat. Add cream and simmer for 5 minutes, then add garlic and cheese and whisk quickly, heating through. Stir in parsley and serve.', 11);
INSERT INTO tb_step (name, id_recipe) VALUES ('In a large mixing bowl, whisk together the flour and the eggs. Gradually add in the milk and water, stirring to combine. Add the salt and butter; beat until smooth.', 12);
INSERT INTO tb_step (name, id_recipe) VALUES ('Heat a lightly oiled griddle or frying pan over medium high heat. Pour or scoop the batter onto the griddle, using approximately 1/4 cup for each crepe. Tilt the pan with a circular motion so that the batter coats the surface evenly.', 12);
INSERT INTO tb_step (name, id_recipe) VALUES ('Cook the crepe for about 2 minutes, until the bottom is light brown. Loosen with a spatula, turn and cook the other side. Serve hot.', 12);
INSERT INTO tb_step (name, id_recipe) VALUES ('Cream room temperature butter with a hand mixer, the paddle attachment of a stand mixer, or a wooden spoon until smooth and fluffy. Gradually beat in confectioners\' sugar until fully incorporated. Beat in vanilla extract.', 13);
INSERT INTO tb_step (name, id_recipe) VALUES ('Pour in milk and beat for an additional 3 to 4 minutes. Add food coloring, if using, and beat for thirty seconds until smooth or until desired color is reached.', 13);
INSERT INTO tb_step (name, id_recipe) VALUES ('Preheat a large skillet or griddle on medium heat.', 14);
INSERT INTO tb_step (name, id_recipe) VALUES ('Lightly butter one side of bread slices. Spread non-buttered sides with Thousand Island dressing. On 4 bread slices, layer 1 slice Swiss cheese, 2 slices corned beef, 1/4 cup sauerkraut and second slice of Swiss cheese. Top with remaining bread slices, buttered sides out.', 14);
INSERT INTO tb_step (name, id_recipe) VALUES ('Grill sandwiches until both sides are golden brown, about 5 minutes per side. Serve hot.', 14);
INSERT INTO tb_step (name, id_recipe) VALUES ('Combine water, beer, and garlic in a multi-functional pressure cooker (such as Instant PotÂ®). Place trivet inside. Place brisket on the trivet and sprinkle spice packet on top. Close and lock the lid. Select high pressure according to manufacturer\'s instructions; set timer for 90 minutes. Allow 10 to 15 minutes for pressure to build.', 15);
INSERT INTO tb_step (name, id_recipe) VALUES ('Release pressure carefully using the quick-release method according to manufacturer\'s instructions, about 5 minutes. Unlock and remove the lid. Transfer brisket to a baking sheet, cover with aluminum foil, and let rest for 15 minutes.', 15);
INSERT INTO tb_step (name, id_recipe) VALUES ('Preheat waffle iron. Beat eggs in large bowl with hand beater until fluffy. Beat in flour, milk, vegetable oil, sugar, baking powder, salt and vanilla, just until smooth.', 16);
INSERT INTO tb_step (name, id_recipe) VALUES ('Spray preheated waffle iron with non-stick cooking spray. Pour mix onto hot waffle iron. Cook until golden brown. Serve hot.', 16);
INSERT INTO tb_step (name, id_recipe) VALUES ('Preheat oven to 325 degrees F (165 degrees C). Grease a 9x5-inch loaf pan.', 17);
INSERT INTO tb_step (name, id_recipe) VALUES ('Combine bananas, sugar, egg, and butter together in a bowl. Mix flour and baking soda together in a separate bowl; stir into banana mixture until batter is just mixed. Stir salt into batter. Pour batter into the prepared loaf pan.', 17);
INSERT INTO tb_step (name, id_recipe) VALUES ('Bake in the preheated oven until a toothpick inserted in the center of the bread comes out clean, about 1 hour.', 17);
INSERT INTO tb_step (name, id_recipe) VALUES ('Preheat the oven to 350 degrees F (175 degrees C). Grease a 9x13-inch pan.', 18);
INSERT INTO tb_step (name, id_recipe) VALUES ('Combine the melted butter, sugar, cocoa powder, vanilla, eggs, flour, baking powder, and salt. Spread the batter into the prepared pan. Decorate with walnut halves, if desired.', 18);
INSERT INTO tb_step (name, id_recipe) VALUES ('Bake in preheated oven for 20 to 30 minutes or until a toothpick inserted in the center comes out with crumbs, not wet. Cool on wire rack.', 18);
INSERT INTO tb_step (name, id_recipe) VALUES ('Bring a large pot of lightly salted water to a boil. Cook elbow macaroni in the boiling water, stirring occasionally until cooked through but firm to the bite, 8 minutes. Drain.', 19);
INSERT INTO tb_step (name, id_recipe) VALUES ('Melt butter in a saucepan over medium heat; stir in flour, salt, and pepper until smooth, about 5 minutes. Slowly pour milk into butter-flour mixture while continuously stirring until mixture is smooth and bubbling, about 5 minutes. Add Cheddar cheese to milk mixture and stir until cheese is melted, 2 to 4 minutes.', 19);
INSERT INTO tb_step (name, id_recipe) VALUES ('Fold macaroni into cheese sauce until coated.', 19);
INSERT INTO tb_step (name, id_recipe) VALUES ('Preheat oven to 425 degrees F (220 degrees C.)', 20);
INSERT INTO tb_step (name, id_recipe) VALUES ('In a saucepan, combine chicken, carrots, peas, and celery. Add water to cover and boil for 15 minutes. Remove from heat, drain and set aside.', 20);
INSERT INTO tb_step (name, id_recipe) VALUES ('In the saucepan over medium heat, cook onions in butter until soft and translucent. Stir in flour, salt, pepper, and celery seed. Slowly stir in chicken broth and milk. Simmer over medium-low heat until thick. Remove from heat and set aside.', 20);
INSERT INTO tb_step (name, id_recipe) VALUES ('Place the chicken mixture in bottom pie crust. Pour hot liquid mixture over. Cover with top crust, seal edges, and cut away excess dough. Make several small slits in the top to allow steam to escape.', 20);
INSERT INTO tb_step (name, id_recipe) VALUES ('Bake in the preheated oven for 30 to 35 minutes, or until pastry is golden brown and filling is bubbly. Cool for 10 minutes before serving.', 20);
INSERT INTO tb_step (name, id_recipe) VALUES ('Place bacon in a large skillet and cook over medium-high heat, turning occasionally, until crispy, 5 to 8 minutes. Drain on paper towels; reserve most of the bacon fat in skillet.', 21);
INSERT INTO tb_step (name, id_recipe) VALUES ('Reduce heat to medium. Cook and stir onions until starting to soften, about 5 minutes. Add potatoes; toss to coat evenly in bacon fat. Pour in water and cover skillet with a large lid. Cook, checking occasionally, until potatoes are tender, about 20 minutes. Add green onions, garlic, and paprika. Season with salt and pepper.', 21);
INSERT INTO tb_step (name, id_recipe) VALUES ('Make 4 wells in the potato mixture using a wooden spoon, revealing the bottom of the skillet. Crack an egg into each well; season with salt and pepper. Sprinkle cooked bacon and Cheddar cheese over the entire skillet. Replace lid; cook until egg whites are set and yolks are still runny, about 5 minutes.', 21);
INSERT INTO tb_step (name, id_recipe) VALUES ('Rinse chicken, and pat dry with paper towels. Place chicken breasts into a slow cooker, and drizzle melted butter all over the chicken breasts, lifting them up to allow butter to flow underneath the chicken. Cover the cooker, set to High, and cook until tender, 4 to 5 hours. With a metal spatula, chop up the chicken into bite-size pieces in the slow cooker.', 22);
INSERT INTO tb_step (name, id_recipe) VALUES ('Place the cream cheese, cream of chicken soup, Italian salad dressing mix, and sherry into a saucepan over medium heat, and bring to a boil, stirring constantly until the cream cheese has melted and the sauce is well combined. Pour the sauce over the chicken in the slow cooker; cover, and cook on High until the flavors have blended and the sauce is bubbling, about 30 more minutes.', 22);
INSERT INTO tb_step (name, id_recipe) VALUES ('Slice chicken breasts horizontally to yield 4 pieces. Place breasts between 2 sheets of plastic wrap and pound to 1/4-inch thin. Place flour on a plate and dredge each breast on both sides in flour.', 23);
INSERT INTO tb_step (name, id_recipe) VALUES ('Melt butter in a large skillet over medium heat. Put chicken into the skillet and season tops with salt and pepper. Turn when nicely browned, about 4 minutes, and season again. Cook second side until browned, about 4 minutes. Transfer breasts to a plate, reserving skillet drippings.', 23);
INSERT INTO tb_step (name, id_recipe) VALUES ('Add wine and lemon juice to the skillet, stirring to scrape up any browned bits. Simmer to reduce and thicken sauce, about 5 minutes more. Add capers and pour sauce over chicken breasts on the plate. Sprinkle with parsley.', 23);
INSERT INTO tb_step (name, id_recipe) VALUES ('In a saucepan, warm the broth over low heat.', 24);
INSERT INTO tb_step (name, id_recipe) VALUES ('Warm 2 tablespoons olive oil in a large saucepan over medium-high heat. Stir in the mushrooms, and cook until soft, about 3 minutes. Remove mushrooms and their liquid, and set aside.', 24);
INSERT INTO tb_step (name, id_recipe) VALUES ('Add 1 tablespoon olive oil to skillet, and stir in the shallots. Cook 1 minute. Add rice, stirring to coat with oil, about 2 minutes. When the rice has taken on a pale, golden color, pour in wine, stirring constantly until the wine is fully absorbed. Add 1/2 cup broth to the rice, and stir until the broth is absorbed. Continue adding broth 1/2 cup at a time, stirring continuously, until the liquid is absorbed and the rice is al dente, about 15 to 20 minutes.', 24);
INSERT INTO tb_step (name, id_recipe) VALUES ('Remove from heat, and stir in mushrooms with their liquid, butter, chives, and parmesan. Season with salt and pepper to taste.', 24);
INSERT INTO tb_step (name, id_recipe) VALUES ('Mix flour, sugar, cocoa powder, baking soda, and salt in a large microwave-safe mug; stir in milk, canola oil, water, and vanilla extract.', 25);
INSERT INTO tb_step (name, id_recipe) VALUES ('Cook in microwave until cake is done in the middle, about 1 minute 45 seconds.', 25);
INSERT INTO tb_step (name, id_recipe) VALUES ('In a medium bowl, cream together butter, white sugar, and brown sugar. Beat in eggs one at a time, then stir in vanilla. Combine flour, baking soda, salt, and cinnamon; stir into the creamed mixture. Mix in oats. Cover, and chill dough for at least one hour.', 26);
INSERT INTO tb_step (name, id_recipe) VALUES ('Preheat the oven to 375 degrees F (190 degrees C). Grease cookie sheets. Roll the dough into walnut sized balls, and place 2 inches apart on cookie sheets. Flatten each cookie with a large fork dipped in sugar.', 26);
INSERT INTO tb_step (name, id_recipe) VALUES ('Bake for 8 to 10 minutes in preheated oven. Allow cookies to cool on baking sheet for 5 minutes before transferring to a wire rack to cool completely.', 26);
INSERT INTO tb_step (name, id_recipe) VALUES ('Preheat oven to 400 degrees F (200 degrees C).', 27);
INSERT INTO tb_step (name, id_recipe) VALUES ('Toss butternut squash with olive oil and garlic in a large bowl. Season with salt and black pepper. Arrange coated squash on a baking sheet.', 27);
INSERT INTO tb_step (name, id_recipe) VALUES ('Roast in the preheated oven until squash is tender and lightly browned, 25 to 30 minutes.', 27);
INSERT INTO tb_step (name, id_recipe) VALUES ('Stir cornstarch, chili powder, salt, paprika, sugar, onion powder, garlic powder, cayenne pepper, and cumin together in a small bowl.', 28);
INSERT INTO tb_step (name, id_recipe) VALUES ('Preheat the oven to 400 degrees F (200 degrees C). Coat a medium-sized casserole dish with cooking spray.', 29);
INSERT INTO tb_step (name, id_recipe) VALUES ('Bring a large pot of lightly salted water to a boil. Cook rigatoni in the boiling water, stirring occasionally until tender yet firm to the bite, about 8 minutes. Drain and set aside.', 29);
INSERT INTO tb_step (name, id_recipe) VALUES ('Mix mayonnaise, garlic, onion, salt, and pepper together in a medium-sized bowl. Add 1 1/4 cup Parmesan cheese; mix well. Mix in cooked pasta and chicken, making sure everything is evenly coated.', 29);
INSERT INTO tb_step (name, id_recipe) VALUES ('Transfer mixture to the prepared casserole dish. Sprinkle with remaining 1/4 cup Parmesan cheese and panko bread crumbs. Cover with aluminum foil.', 29);
INSERT INTO tb_step (name, id_recipe) VALUES ('Bake in the preheated oven for 30 minutes. Remove from the oven, uncover, and replace in the oven. Bake until chicken is no longer pink in the center and juices run clear, about 10 minutes more. Let stand for 1 to 2 minutes before dishing out.', 29);
INSERT INTO tb_step (name, id_recipe) VALUES ('Preheat oven to 275 degrees F (135 degrees C).', 30);
INSERT INTO tb_step (name, id_recipe) VALUES ('Discard any flavoring packet from corned beef. Brush brisket with browning sauce on both sides. Heat vegetable oil in a large skillet over medium-high heat and brown brisket on both sides in the hot oil, 5 to 8 minutes per side.', 30);
INSERT INTO tb_step (name, id_recipe) VALUES ('Place brisket on a rack set in a roasting pan. Scatter onion and garlic slices over brisket and add water to roasting pan. Cover pan tightly with aluminum foil.', 30);
INSERT INTO tb_step (name, id_recipe) VALUES ('Roast in the preheated oven until meat is tender, about 6 hours.', 30);
INSERT INTO tb_step (name, id_recipe) VALUES ('Place mint leaves and 1 lime wedge into a sturdy glass. Use a muddler to crush the mint and lime to release the mint oils and lime juice. Add 2 more lime wedges and the sugar, and muddle again to release the lime juice. Do not strain the mixture. Fill the glass almost to the top with ice. Pour the rum over the ice, and fill the glass with carbonated water. Stir, taste, and add more sugar if desired. Garnish with the remaining lime wedge.', 31);
INSERT INTO tb_step (name, id_recipe) VALUES ('Mix mayonnaise, lemon juice, honey, and poppy seeds together in a medium bowl. Add chicken, strawberries, pecans, celery, green onions, parsley; toss to combine. Season with salt and pepper.', 32);
INSERT INTO tb_step (name, id_recipe) VALUES ('In a large bowl, mix flour, sugar, baking powder and salt. Make a well in the center, and pour in milk, egg and oil. Mix until smooth.', 33);
INSERT INTO tb_step (name, id_recipe) VALUES ('Heat a lightly oiled griddle or frying pan over medium high heat. Pour or scoop the batter onto the griddle, using approximately 1/4 cup for each pancake. Brown on both sides and serve hot.', 33);
INSERT INTO tb_step (name, id_recipe) VALUES ('Preheat the oven to 325 degrees F (165 degrees C). Grease cookie sheets or line with parchment paper.', 34);
INSERT INTO tb_step (name, id_recipe) VALUES ('Sift together the flour, baking soda and salt; set aside.', 34);
INSERT INTO tb_step (name, id_recipe) VALUES ('In a medium bowl, cream together the melted butter, brown sugar and white sugar until well blended. Beat in the vanilla, egg, and egg yolk until light and creamy. Mix in the sifted ingredients until just blended. Stir in the chocolate chips by hand using a wooden spoon. Drop cookie dough 1/4 cup at a time onto the prepared cookie sheets. Cookies should be about 3 inches apart.', 34);
INSERT INTO tb_step (name, id_recipe) VALUES ('Bake for 15 to 17 minutes in the preheated oven, or until the edges are lightly toasted. Cool on baking sheets for a few minutes before transferring to wire racks to cool completely.', 34);
INSERT INTO tb_step (name, id_recipe) VALUES ('Preheat oven to 350 degrees F (175 degrees C).', 35);
INSERT INTO tb_step (name, id_recipe) VALUES ('Rinse pork chops, pat dry, and season with garlic powder and seasoning salt to taste. Place the beaten eggs in a small bowl. Dredge the pork chops lightly in flour, dip in the egg, and coat liberally with bread crumbs.', 35);
INSERT INTO tb_step (name, id_recipe) VALUES ('Heat the oil in a medium skillet over medium-high heat. Fry the pork chops 5 minutes per side, or until the breading appears well browned. Transfer the chops to a 9x13 inch baking dish, and cover with foil.', 35);
INSERT INTO tb_step (name, id_recipe) VALUES ('Bake in the preheated oven for 1 hour. While baking, combine the cream of mushroom soup, milk and white wine in a medium bowl. After the pork chops have baked for an hour, cover them with the soup mixture. Replace foil, and bake for another 30 minutes.', 35);
INSERT INTO tb_step (name, id_recipe) VALUES ('Drain and reserve liquid from salmon. Mix egg, onion, bread crumbs and salmon together.', 36);
INSERT INTO tb_step (name, id_recipe) VALUES ('Make into patties. If mixture is too dry to form into patties, add reserved liquid from salmon.', 36);
INSERT INTO tb_step (name, id_recipe) VALUES ('In a frying pan, heat olive oil. Place patties in pan. Brown on each side, turning gently. Drain on paper towels and serve.', 36);
INSERT INTO tb_step (name, id_recipe) VALUES ('Preheat oven to 350 degrees F (175 degrees C).', 37);
INSERT INTO tb_step (name, id_recipe) VALUES ('Cook and stir ground beef in a large skillet over medium heat until no longer pink and completely browned, 7 to 10 minutes; season with salt and black pepper. Stir cream of mushroom soup into the cooked ground beef; pour the mixture into a 9x13-inch baking dish. Layer tater tots evenly over the ground beef mixture; top with Cheddar cheese.', 37);
INSERT INTO tb_step (name, id_recipe) VALUES ('Bake until tater tots are golden brown and hot, 30 to 45 minutes.', 37);
INSERT INTO tb_step (name, id_recipe) VALUES ('Bring a medium saucepan of water to boil. Boil rice vermicelli 3 to 5 minutes, or until al dente, and drain.', 38);
INSERT INTO tb_step (name, id_recipe) VALUES ('Fill a large bowl with warm water. Dip one wrapper into the hot water for 1 second to soften. Lay wrapper flat. In a row across the center, place 2 shrimp halves, a handful of vermicelli, basil, mint, cilantro and lettuce, leaving about 2 inches uncovered on each side. Fold uncovered sides inward, then tightly roll the wrapper, beginning at the end with the lettuce. Repeat with remaining ingredients.', 38);
INSERT INTO tb_step (name, id_recipe) VALUES ('In a small bowl, mix the fish sauce, water, lime juice, garlic, sugar and chili sauce.', 38);
INSERT INTO tb_step (name, id_recipe) VALUES ('In another small bowl, mix the hoisin sauce and peanuts.', 38);
INSERT INTO tb_step (name, id_recipe) VALUES ('Serve rolled spring rolls with the fish sauce and hoisin sauce mixtures.', 38);
INSERT INTO tb_step (name, id_recipe) VALUES ('Stir the mayonnaise, relish, mustard, and lemon juice together in a bowl.', 39);
INSERT INTO tb_step (name, id_recipe) VALUES ('Cook and stir ground beef, onion, garlic powder, and oregano in a large skillet over medium heat until onion is tender and beef is browned, about 10 minutes. Add tomato sauce and sugar; simmer until sauce is heated through, about 10 minutes.', 40);
INSERT INTO tb_step (name, id_recipe) VALUES ('Bring a large pot of lightly salted water to a boil. Cook lasagna in the boiling water, stirring occasionally until cooked through but firm to the bite, 8 to 10 minutes. Drain.', 40);
INSERT INTO tb_step (name, id_recipe) VALUES ('Preheat oven to 375 degrees F (190 degrees C). Grease a 9x13-inch casserole dish.', 40);
INSERT INTO tb_step (name, id_recipe) VALUES ('Mix ricotta cheese, egg, parsley, and 3/4 cup Parmesan cheese together in a bowl.', 40);
INSERT INTO tb_step (name, id_recipe) VALUES ('Place a lasagna noodle on a work surface. Spoon one layer of ricotta cheese mixture, one layer meat sauce, and one layer mozzarella cheese atop the lasagna noodle; top with another lasagna noodle. Roll up the stuffed lasagna noodles in the jelly-roll style; place in the prepared casserole dish. Repeat with remaining lasagna noodles, ricotta cheese mixture, meat sauce, and mozzarella cheese. Spread a spoonful of meat sauce atop each lasagna roll; top with remaining mozzarella cheese and 1/4 cup Parmesan cheese.', 40);
INSERT INTO tb_step (name, id_recipe) VALUES ('Bake in the preheated oven until cheese is melted and bubbling, 30 to 35 minutes.', 40);
INSERT INTO tb_step (name, id_recipe) VALUES ('Preheat an oven to 350 degrees F (175 degrees C). Line a non insulated cookie sheet with parchment paper.', 41);
INSERT INTO tb_step (name, id_recipe) VALUES ('With a knife or kitchen shears carefully remove the leaves from the thick stems and tear into bite size pieces. Wash and thoroughly dry kale with a salad spinner. Drizzle kale with olive oil and sprinkle with seasoning salt.', 41);
INSERT INTO tb_step (name, id_recipe) VALUES ('Bake until the edges brown but are not burnt, 10 to 15 minutes.', 41);
INSERT INTO tb_step (name, id_recipe) VALUES ('Preheat oven to 350 degrees F (175 degrees C).', 42);
INSERT INTO tb_step (name, id_recipe) VALUES ('Crush garlic with rosemary, salt and pepper, making a paste. Pierce meat with a sharp knife in several places and press the garlic paste into the openings. Rub the meat with the remaining garlic mixture and olive oil.', 42);
INSERT INTO tb_step (name, id_recipe) VALUES ('Place pork loin into oven, turning and basting with pan liquids. Cook until the pork is no longer pink in the center, about 1 hour. An instant-read thermometer inserted into the center should read 145 degrees F (63 degrees C). Remove roast to a platter. Heat the wine in the pan and stir to loosen browned bits of food on the bottom. Serve with pan juices.', 42);
INSERT INTO tb_step (name, id_recipe) VALUES ('In a large bowl, whip cream until stiff peaks are just about to form. Beat in vanilla and sugar until peaks form. Make sure not to over-beat, cream will then become lumpy and butter-like.', 43);
INSERT INTO tb_step (name, id_recipe) VALUES ('Preheat the oven to 325 degrees F (165 degrees C).', 44);
INSERT INTO tb_step (name, id_recipe) VALUES ('In a large bowl, cream together the butter, brown sugar, and white sugar until smooth. Beat in eggs one at a time, then stir in vanilla. Combine the flour, baking soda, and salt; stir into the creamed mixture until just blended. Mix in the quick oats, walnuts, and chocolate chips. Drop by heaping spoonfuls onto ungreased baking sheets.', 44);
INSERT INTO tb_step (name, id_recipe) VALUES ('Bake for 12 minutes in the preheated oven. Allow cookies to cool on baking sheet for 5 minutes before transferring to a wire rack to cool completely.', 44);
INSERT INTO tb_step (name, id_recipe) VALUES ('Combine flour, white sugar, baking powder and salt. In a separate bowl, mix together egg, milk, vegetable oil and bananas.', 45);
INSERT INTO tb_step (name, id_recipe) VALUES ('Stir flour mixture into banana mixture; batter will be slightly lumpy.', 45);
INSERT INTO tb_step (name, id_recipe) VALUES ('Heat a lightly oiled griddle or frying pan over medium high heat. Pour or scoop the batter onto the griddle, using approximately 1/4 cup for each pancake. Cook until pancakes are golden brown on both sides; serve hot.', 45);
INSERT INTO tb_step (name, id_recipe) VALUES ('To Marinate: Mix 2 cups wine, tarragon, parsley and oil in a nonporous glass dish or bowl and. Add chicken, and toss to coat. Cover dish or bowl, and refrigerate to marinate for at least 4 hours.', 46);
INSERT INTO tb_step (name, id_recipe) VALUES ('Remove chicken from marinade, and pour marinade into a large skillet over medium high heat. Cut chicken breasts in half vertically and quickly flatten each piece to 1/2 inch thickness with a meat mallet. Layer each piece a slice of cheese, slice of ham, and a sprinkle of brown sugar. Roll and secure with toothpicks, then brown in skillet with marinade.', 46);
INSERT INTO tb_step (name, id_recipe) VALUES ('Preheat oven to 325 degrees F (165 degrees C).', 46);
INSERT INTO tb_step (name, id_recipe) VALUES ('Place browned rollups in a 9x13 inch baking dish and pour a small amount of marinade over chicken, reserving at least 3 tablespoons in the skillet. Sprinkle chicken with salt and pepper to taste and a small amount of brown sugar.', 46);
INSERT INTO tb_step (name, id_recipe) VALUES ('Bake chicken rollups in preheated oven for about 30 minutes, or until cooked through and chicken juices run clear.', 46);
INSERT INTO tb_step (name, id_recipe) VALUES ('To Make Sauce: Meanwhile, add flour to skillet with reserved marinade. Add 2 cups wine, stirring constantly, letting sauce thicken. Then stir in mustard, honey, a bit of brown sugar and horseradish. Season with salt and pepper to taste, heat through and serve with chicken rollups.', 46);
INSERT INTO tb_step (name, id_recipe) VALUES ('Place steaks side by side in large casserole dish. Add orange juice, cider vinegar, and Worcestershire sauce. Refrigerate, uncovered, for 45 minutes.', 47);
INSERT INTO tb_step (name, id_recipe) VALUES ('Preheat the oven to 425 degrees F (220 degrees C).', 47);
INSERT INTO tb_step (name, id_recipe) VALUES ('Remove casserole dish from refrigerator. Cover steaks with plastic wrap and let reach room temperature, at least 15 minutes.', 47);
INSERT INTO tb_step (name, id_recipe) VALUES ('Heat olive oil in a cast-iron skillet over high heat.', 47);
INSERT INTO tb_step (name, id_recipe) VALUES ('Place steaks on a clean work surface and generously rub with steak seasoning and black pepper.', 47);
INSERT INTO tb_step (name, id_recipe) VALUES ('Cook steaks in the hot skillet until lightly browned on the bottom, 2 1/2 minutes. Flip and cook until browned on the other side and red in the center, about 2 minutes more. Place skillet, with steaks, into the oven.', 47);
INSERT INTO tb_step (name, id_recipe) VALUES ('Bake in the preheated oven until steaks are firm and reddish-pink to lightly pink in the center, 8 to 10 minutes. An instant-read thermometer inserted into the center should read from 130 degrees F (54 degrees C) to 140 degrees F (60 degrees C).', 47);
INSERT INTO tb_step (name, id_recipe) VALUES ('Remove steaks from oven; season with salt. Let sit for 5 minutes before serving.', 47);
INSERT INTO tb_step (name, id_recipe) VALUES ('Combine the hot sauce, butter, vinegar, Worcestershire sauce, cayenne pepper, garlic powder, and salt in a pot and place over medium heat. Bring to a simmer while stirring with a whisk. As soon as the liquid begins to bubble on the sides of the pot, remove from heat, stir with the whisk, and set aside for use.', 48);
INSERT INTO tb_step (name, id_recipe) VALUES ('Bring 1 1/2 cups water to a boil in a saucepan; stir rice into boiling water. Reduce heat to low, cover, and simmer for 20 minutes.', 49);
INSERT INTO tb_step (name, id_recipe) VALUES ('In a clean saucepan, combine 1 1/2 cups cooked rice, 1 1/2 cups milk, sugar and salt. Cook over medium heat until thick and creamy, 15 to 20 minutes. Stir in remaining 1/2 cup milk, beaten egg, and raisins; cook 2 minutes more, stirring constantly. Remove from heat and stir in butter and vanilla.', 49);
INSERT INTO tb_step (name, id_recipe) VALUES ('Heat the oil in a deep-fryer to 365 degrees F (185 degrees C).', 50);
INSERT INTO tb_step (name, id_recipe) VALUES ('Separate the onion slices into rings, and set aside. In a small bowl, stir together the flour, baking powder and salt.', 50);
INSERT INTO tb_step (name, id_recipe) VALUES ('Dip the onion slices into the flour mixture until they are all coated; set aside. Whisk the egg and milk into the flour mixture using a fork. Dip the floured rings into the batter to coat, then place on a wire rack to drain until the batter stops dripping. The wire rack may be placed over a sheet of aluminum foil for easier clean up. Spread the bread crumbs out on a plate or shallow dish. Place rings one at a time into the crumbs, and scoop the crumbs up over the ring to coat. Give it a hard tap as you remove it from the crumbs. The coating should cling very well. Repeat with remaining rings.', 50);
INSERT INTO tb_step (name, id_recipe) VALUES ('Deep fry the rings a few at a time for 2 to 3 minutes, or until golden brown. Remove to paper towels to drain. Season with seasoning salt, and serve.', 50);
INSERT INTO tb_step (name, id_recipe) VALUES ('In a large pot of salted boiling water, cook pasta until al dente, rinse under cold water and drain.', 51);
INSERT INTO tb_step (name, id_recipe) VALUES ('Whisk together the salad spice mix and Italian dressing.', 51);
INSERT INTO tb_step (name, id_recipe) VALUES ('In a salad bowl, combine the pasta, cherry tomatoes, bell peppers and olives. Pour dressing over salad; toss and refrigerate overnight.', 51);
INSERT INTO tb_step (name, id_recipe) VALUES ('In a small saucepan, combine sugar and 1 cup water. Bring to boil and stir to dissolve sugar. Allow to cool to room temperature, then cover and refrigerate until chilled.', 52);
INSERT INTO tb_step (name, id_recipe) VALUES ('Remove seeds from lemon juice, but leave pulp. In pitcher, stir together chilled syrup, lemon juice and remaining 7 cups water.', 52);
INSERT INTO tb_step (name, id_recipe) VALUES ('Heat a large nonstick skillet over medium-high heat. Cook and stir hamburger, bell pepper, and onion in the hot skillet until meat is browned and vegetables are soft, 5 to 7 minutes. Add garlic; cook until fragrant, about 30 seconds. Drain and discard grease.', 53);
INSERT INTO tb_step (name, id_recipe) VALUES ('Add condensed soup, vegetables, Cheddar cheese, water, Worcestershire sauce, soy sauce, garlic salt, chili powder, salt, and pepper. Cook, stirring occasionally, until vegetables are hot and cheese is melted, 10 to 15 minutes.', 53);
INSERT INTO tb_step (name, id_recipe) VALUES ('Serve stew over warm brown rice.', 53);
INSERT INTO tb_step (name, id_recipe) VALUES ('Soak Kaiser roll in a bowl of water for 10 minutes. Drain, squeezing out excess water, and crumble into a large bowl.', 54);
INSERT INTO tb_step (name, id_recipe) VALUES ('Mix crumbled roll with ground beef, ground pork, onion, parsley, egg, paprika, salt, and black pepper until well blended. Shape meat into large flattened meatballs.', 54);
INSERT INTO tb_step (name, id_recipe) VALUES ('Heat a large nonstick skillet over medium heat; fry meatballs until browned and no longer pink in the center, about 5 minutes per side.', 54);
INSERT INTO tb_step (name, id_recipe) VALUES ('Preheat the oven to 400 degrees F (200 degrees C).', 55);
INSERT INTO tb_step (name, id_recipe) VALUES ('Place potatoes, garlic powder, black pepper, celery seed, paprika, cayenne pepper, and salt into a zip-top bag. Shake to coat evenly.', 55);
INSERT INTO tb_step (name, id_recipe) VALUES ('Pour entire contents of bag onto a large baking sheet and spread evenly. Drizzle with olive oil.', 55);
INSERT INTO tb_step (name, id_recipe) VALUES ('Bake in the preheated oven until potatoes reach desired crispness, turning frequently to brown evenly, 40 to 45 minutes.', 55);
INSERT INTO tb_step (name, id_recipe) VALUES ('Remove from the oven. Let cool slightly and pour into a serving dish.', 55);
INSERT INTO tb_step (name, id_recipe) VALUES ('Melt butter in a medium, non-stick saucepan over medium heat. Add cream cheese and garlic powder, stirring with wire whisk until smooth. Add milk, a little at a time, whisking to smooth out lumps. Stir in Parmesan and pepper. Remove from heat when sauce reaches desired consistency. Sauce will thicken rapidly, thin with milk if cooked too long. Toss with hot pasta to serve.', 56);
INSERT INTO tb_step (name, id_recipe) VALUES ('Preheat oven to 350 degrees F (175 degrees C). Grease a 9x5 inch loaf pan.', 57);
INSERT INTO tb_step (name, id_recipe) VALUES ('In a large bowl, stir together the melted butter and sugar. Add the eggs and vanilla, mix well. Combine the flour, baking soda and salt, stir into the butter mixture until smooth. Finally, fold in the sour cream, walnuts and bananas. Spread evenly into the prepared pan.', 57);
INSERT INTO tb_step (name, id_recipe) VALUES ('Bake at 350 degrees F (175 degrees C) for 60 minutes, or until a toothpick inserted into the center of the loaf comes out clean. Cool loaf in the pan for 10 minutes before removing to a wire rack to cool completely.', 57);
INSERT INTO tb_step (name, id_recipe) VALUES ('Place ingredients in the pan of the bread machine in the order recommended by the manufacturer. Select dough cycle; press Start.', 58);
INSERT INTO tb_step (name, id_recipe) VALUES ('After the dough has doubled in size turn it out onto a lightly floured surface, cover and let rest for 10 minutes. In a small bowl, combine brown sugar and cinnamon.', 58);
INSERT INTO tb_step (name, id_recipe) VALUES ('Roll dough into a 16x21-inch rectangle. Spread dough with 1/3 cup butter and sprinkle evenly with sugar/cinnamon mixture. Roll up dough and cut into 12 rolls. Place rolls in a lightly greased 9x13 inch baking pan. Cover and let rise until nearly doubled, about 30 minutes. Meanwhile, preheat oven to 400 degrees F (200 degrees C).', 58);
INSERT INTO tb_step (name, id_recipe) VALUES ('Bake rolls in preheated oven until golden brown, about 15 minutes. While rolls are baking, beat together cream cheese, 1/4 cup butter, confectioners\' sugar, vanilla extract and salt. Spread frosting on warm rolls before serving.', 58);
INSERT INTO tb_step (name, id_recipe) VALUES ('Bring a large pot of salted water to a boil. Peel potatoes and add to pot. Cook until tender but still firm, about 15 minutes. Drain, cool and mash with a fork or potato masher.', 59);
INSERT INTO tb_step (name, id_recipe) VALUES ('Combine 1 cup mashed potato, flour and egg in a large bowl. Knead until dough forms a ball. Shape small portions of the dough into long 'snakes'. On a floured surface, cut snakes into half-inch pieces.', 59);
INSERT INTO tb_step (name, id_recipe) VALUES ('Bring a large pot of lightly salted water to a boil. Drop in gnocchi and cook for 3 to 5 minutes or until gnocchi have risen to the top; drain and serve.', 59);
INSERT INTO tb_step (name, id_recipe) VALUES ('Melt butter with olive oil in an 8 quart stock pot on medium heat. Add onions and continually stir until tender and translucent. Do not brown the onions.', 60);
INSERT INTO tb_step (name, id_recipe) VALUES ('Add beef broth, sherry and thyme. Season with salt and pepper, and simmer for 30 minutes.', 60);
INSERT INTO tb_step (name, id_recipe) VALUES ('Heat the oven broiler.', 60);
INSERT INTO tb_step (name, id_recipe) VALUES ('Ladle soup into oven safe serving bowls and place one slice of bread on top of each (bread may be broken into pieces if you prefer). Layer each slice of bread with a slice of provolone, 1/2 slice diced Swiss and 1 tablespoon Parmesan cheese. Place bowls on cookie sheet and broil in the preheated oven until cheese bubbles and browns slightly.', 60);
INSERT INTO tb_step (name, id_recipe) VALUES ('In a large bowl, mix together 1/3 cup condensed French onion soup with ground beef, bread crumbs, egg, salt and black pepper. Shape into 6 oval patties.', 61);
INSERT INTO tb_step (name, id_recipe) VALUES ('In a large skillet over medium-high heat, brown both sides of patties. Pour off excess fat.', 61);
INSERT INTO tb_step (name, id_recipe) VALUES ('In a small bowl, blend flour and remaining soup until smooth. Mix in ketchup, water, Worcestershire sauce and mustard powder. Pour over meat in skillet. Cover, and cook for 20 minutes, stirring occasionally.', 61);
INSERT INTO tb_step (name, id_recipe) VALUES ('Preheat oven to 425 degrees F (220 degrees C).', 62);
INSERT INTO tb_step (name, id_recipe) VALUES ('In a bowl, mix the mayonnaise, Parmesan cheese, and garlic powder. Place bread crumbs in a separate bowl. Dip chicken into the mayonnaise mixture, then into the bread crumbs to coat. Arrange coated chicken on a baking sheet.', 62);
INSERT INTO tb_step (name, id_recipe) VALUES ('Bake 20 minutes in the preheated oven, or until chicken juices run clear and coating is golden brown.', 62);
INSERT INTO tb_step (name, id_recipe) VALUES ('Preheat oven to 400 degrees F (200 degrees C).', 63);
INSERT INTO tb_step (name, id_recipe) VALUES ('Place 2 tablespoons butter in a microwave-safe bowl; melt in microwave on high, about 30 seconds. Stir buttery round crackers into melted butter.', 63);
INSERT INTO tb_step (name, id_recipe) VALUES ('Place remaining 2 tablespoons butter in a 7x11-inch baking dish. Melt in the preheated oven, 1 to 3 minutes. Remove dish from oven.', 63);
INSERT INTO tb_step (name, id_recipe) VALUES ('Coat both sides of cod in melted butter in the baking dish.', 63);
INSERT INTO tb_step (name, id_recipe) VALUES ('Bake cod in the preheated oven for 10 minutes. Remove from oven; top with lemon juice, wine, and cracker mixture. Place back in oven and bake until fish is opaque and flakes easily with a fork, about 10 more minutes.', 63);
INSERT INTO tb_step (name, id_recipe) VALUES ('Garnish baked cod with parsley and green onion. Serve with lemon wedges.', 63);
INSERT INTO tb_step (name, id_recipe) VALUES ('Pour the simple syrup, water, and bitters into a whiskey glass. Stir to combine, then place the ice cubes in the glass. Pour bourbon over the ice and garnish with the orange slice and maraschino cherry.', 64);
INSERT INTO tb_step (name, id_recipe) VALUES ('Bring water and salt to a boil in a large saucepan; pour polenta slowly into boiling water, whisking constantly until all polenta is stirred in and there are no lumps.', 65);
INSERT INTO tb_step (name, id_recipe) VALUES ('Reduce heat to low and simmer, whisking often, until polenta starts to thicken, about 5 minutes. Polenta mixture should still be slightly loose. Cover and cook for 30 minutes, whisking every 5 to 6 minutes. When polenta is too thick to whisk, stir with a wooden spoon. Polenta is done when texture is creamy and the individual grains are tender.', 65);
INSERT INTO tb_step (name, id_recipe) VALUES ('Turn off heat and gently stir 2 tablespoons butter into polenta until butter partially melts; mix 1/2 cup Parmigiano-Reggiano cheese into polenta until cheese has melted. Cover and let stand 5 minutes to thicken; stir and taste for salt before transferring to a serving bowl. Top polenta with remaining 1 tablespoon butter and about 1 tablespoon freshly grated Parmigiano-Reggiano cheese for garnish.', 65);
INSERT INTO tb_step (name, id_recipe) VALUES ('Preheat oven to 400 degrees F (200 degrees C). Grease sides and bottom of a casserole dish.', 66);
INSERT INTO tb_step (name, id_recipe) VALUES ('Stir chicken, water, rice, cream of chicken soup, cream of celery soup, and cream of mushroom soup together in the prepared casserole dish; season with salt and pepper. Arrange butter evenly over the top of the chicken mixture.', 66);
INSERT INTO tb_step (name, id_recipe) VALUES ('Bake in preheated oven until the rice is tender and the chicken is cooked through, 1 hour to 75 minutes. Cool 15 minutes before serving.', 66);
INSERT INTO tb_step (name, id_recipe) VALUES ('Sprinkle the chicken breasts with 2 teaspoons salt.', 67);
INSERT INTO tb_step (name, id_recipe) VALUES ('Heat the oil in a large skillet over high heat; partially cook the chicken in the hot oil in batches until completely browned. Transfer the browned chicken breasts to a plate and set aside.', 67);
INSERT INTO tb_step (name, id_recipe) VALUES ('Reduce the heat under the skillet to medium-high; add the onion, garlic, and ginger to the oil remaining in the skillet and cook and stir until the onion turns translucent, about 8 minutes. Stir the curry powder, cumin, turmeric, coriander, cayenne, and 1 tablespoon of water into the onion mixture; allow to heat together for about 1 minute while stirring. Mix the tomatoes, yogurt, 1 tablespoon chopped cilantro, and 1 teaspoon salt into the mixture. Return the chicken breast to the skillet along with any juices on the plate. Pour 1/2 cup water into the mixture; bring to a boil, turning the chicken to coat with the sauce. Sprinkle the garam masala and 1 tablespoon cilantro over the chicken.', 67);
INSERT INTO tb_step (name, id_recipe) VALUES ('Cover the skillet and simmer until the chicken breasts are no longer pink in the center and the juices run clear, about 20 minutes. An instant-read thermometer inserted into the center should read at least 165 degrees F (74 degrees C). Sprinkle with lemon juice to serve.', 67);
INSERT INTO tb_step (name, id_recipe) VALUES ('Combine cooked rice, 1 1/2 cups milk, and salt in a saucepan over medium heat; cook and stir until thick and creamy, 15 to 20 minutes. Stir remaining 1/2 cup milk, golden raisins, beaten egg, and white sugar into the rice mixture; stirring continually. Continue cooking until egg is set, 2 to 3 minutes. Remove saucepan from heat; stir butter and vanilla extract into the pudding.', 68);
INSERT INTO tb_step (name, id_recipe) VALUES ('Bring a large pot of lightly salted water to a boil. Cook spaghetti in the boiling water, stirring occasionally until cooked through but firm to the bite, about 12 minutes. Drain and transfer to a pasta bowl.', 69);
INSERT INTO tb_step (name, id_recipe) VALUES ('Combine garlic and olive oil in a cold skillet. Cook over medium heat to slowly toast garlic, about 10 minutes. Reduce heat to medium-low when olive oil begins to bubble. Cook and stir until garlic is golden brown, about another 5 minutes. Remove from heat.', 69);
INSERT INTO tb_step (name, id_recipe) VALUES ('Stir red pepper flakes, black pepper, and salt into the pasta. Pour in olive oil and garlic, and sprinkle on Italian parsley and half of the Parmigiano-Reggiano cheese; stir until combined.', 69);
INSERT INTO tb_step (name, id_recipe) VALUES ('Serve pasta topped with the remaining Parmigiano-Reggiano cheese.', 69);
INSERT INTO tb_step (name, id_recipe) VALUES ('Combine eggs, milk, bread crumbs, onion, salt, sage, and mushrooms in a large bowl. Crumble ground beef over mixture and stir well to combine. Shape into a round loaf; place in a 5-quart slow cooker. Cover and cook on Low until a meat thermometer reads 160 degrees F (71 degrees C), 5 to 6 hours.', 70);
INSERT INTO tb_step (name, id_recipe) VALUES ('Whisk ketchup, brown sugar, mustard, and Worcestershire sauce in a small bowl; spoon sauce over meat loaf. Return to slow cooker and cook on Low until heated through, about 15 minutes. Let stand 10 minutes before cutting.', 70);
INSERT INTO tb_step (name, id_recipe) VALUES ('Preheat oven to 350 degrees F (175 degrees C).', 71);
INSERT INTO tb_step (name, id_recipe) VALUES ('Bake potatoes in preheated oven for 1 hour.', 71);
INSERT INTO tb_step (name, id_recipe) VALUES ('Meanwhile, place bacon in a large, deep skillet. Cook over medium high heat until evenly brown. Drain, crumble and set aside.', 71);
INSERT INTO tb_step (name, id_recipe) VALUES ('When potatoes are done allow them to cool for 10 minutes. Slice potatoes in half lengthwise and scoop the flesh into a large bowl; save skins. To the potato flesh add sour cream, milk, butter, salt, pepper, 1/2 cup cheese and 1/2 the green onions. Mix with a hand mixer until well blended and creamy. Spoon the mixture into the potato skins. Top each with remaining cheese, green onions and bacon.', 71);
INSERT INTO tb_step (name, id_recipe) VALUES ('Bake for another 15 minutes.', 71);
INSERT INTO tb_step (name, id_recipe) VALUES ('Preheat oven to 350 degrees F (175 degrees C). Line baking sheets with parchment paper.', 72);
INSERT INTO tb_step (name, id_recipe) VALUES ('Combine the peanut butter, white sugar and egg. Mix until smooth.', 72);
INSERT INTO tb_step (name, id_recipe) VALUES ('Drop spoonfuls of dough onto the prepared baking sheet. Bake at 350 degrees F (175 degrees C) for 6 to 8 minutes. Do not overbake! These cookies are best when they are still soft and just barely brown on the bottoms.', 72);
INSERT INTO tb_step (name, id_recipe) VALUES ('Place almonds in a frying pan. Toast over medium-high heat, shaking frequently. Watch carefully, as they burn easily.', 73);
INSERT INTO tb_step (name, id_recipe) VALUES ('In a medium bowl, mix together mayonnaise, lemon juice, and pepper. Toss with chicken, almonds, and celery.', 73);
INSERT INTO tb_step (name, id_recipe) VALUES ('In a bowl, toss together the tomatoes, basil, Parmesan cheese, and garlic. Mix in the balsamic vinegar, olive oil, kosher salt, and pepper. Serve on toasted bread slices.', 74);
INSERT INTO tb_step (name, id_recipe) VALUES ('Rinse shredded potatoes until water is clear, then drain and squeeze dry. Place shreds in a bowl, and mix in the onion, flour and egg until evenly distributed.', 75);
INSERT INTO tb_step (name, id_recipe) VALUES ('Heat about 1/4 inch of oil in a large heavy skillet over medium-high heat. When oil is sizzling hot, place potatoes into the pan in a 1/2 inch thick layer. Cover the whole bottom of the pan, or make separate piles like pancakes. Cook until nicely browned on the bottom, then flip over and brown on the other side. It should take at least 5 minutes per side. If you are cooking them in one big piece, it can be cut into quarters for easier flipping.', 75);
INSERT INTO tb_step (name, id_recipe) VALUES ('Remove from pan, and drain on paper towels. Season with salt and pepper and serve immediately.', 75);
INSERT INTO tb_step (name, id_recipe) VALUES ('Place carrots, onions, tomatoes, cabbage, green beans, peppers, and celery in a large pot. Add onion soup mix, tomato juice, beef broth, and enough water to cover vegetables. Simmer until vegetables are tender. May be stored in the refrigerator for several days.', 76);
INSERT INTO tb_step (name, id_recipe) VALUES ('Season the tuna steaks with salt and cayenne pepper.', 77);
INSERT INTO tb_step (name, id_recipe) VALUES ('Melt the butter with the olive oil in a skillet over medium-high heat. Cook the peppercorns in the mixture until they soften and pop, about 5 minutes. Gently place the seasoned tuna in the skillet and cook to desired doneness, 1 1/2 minutes per side for rare.', 77);
INSERT INTO tb_step (name, id_recipe) VALUES ('Bring a large pot of lightly salted water to a boil. Add ziti pasta, and cook until al dente, about 8 minutes; drain.', 78);
INSERT INTO tb_step (name, id_recipe) VALUES ('In a large skillet, brown onion and ground beef over medium heat. Add spaghetti sauce, and simmer 15 minutes.', 78);
INSERT INTO tb_step (name, id_recipe) VALUES ('Preheat the oven to 350 degrees F (175 degrees C). Butter a 9x13 inch baking dish. Layer as follows: 1/2 of the ziti, Provolone cheese, sour cream, 1/2 sauce mixture, remaining ziti, mozzarella cheese and remaining sauce mixture. Top with grated Parmesan cheese.', 78);
INSERT INTO tb_step (name, id_recipe) VALUES ('Bake for 30 minutes in the preheated oven, or until cheeses are melted.', 78);
INSERT INTO tb_step (name, id_recipe) VALUES ('Gently rinse liver slices under cold water, and place in a medium bowl. Pour in enough milk to cover. Let stand while preparing onions. (I like to soak up to an hour or two - whatever you have time for.) This step is SO important in taking the bitter taste of the liver out.', 79);
INSERT INTO tb_step (name, id_recipe) VALUES ('Melt 2 tablespoons of butter in a large skillet over medium heat. Separate onion rings, and saute them in butter until soft. Remove onions, and melt remaining butter in the skillet. Season the flour with salt and pepper, and put it in a shallow dish or on a plate. Drain milk from liver, and coat slices in the flour mixture.', 79);
INSERT INTO tb_step (name, id_recipe) VALUES ('When the butter has melted, turn the heat up to medium-high, and place the coated liver slices in the pan. Cook until nice and brown on the bottom. Turn, and cook on the other side until browned. Add onions, and reduce heat to medium. Cook a bit longer to taste. Our family prefers the liver to just barely retain a pinkness on the inside when you cut to check. Enjoy!', 79);
INSERT INTO tb_step (name, id_recipe) VALUES ('Preheat grill for high heat.', 80);
INSERT INTO tb_step (name, id_recipe) VALUES ('In a large bowl, mix the ground beef, egg, bread crumbs, evaporated milk, Worcestershire sauce, cayenne pepper, and garlic using your hands. Form the mixture into 8 hamburger patties.', 80);
INSERT INTO tb_step (name, id_recipe) VALUES ('Lightly oil the grill grate. Grill patties 5 minutes per side, or until well done.', 80);
INSERT INTO tb_step (name, id_recipe) VALUES ('Preheat oven to 425 degrees F (220 degrees C). Lightly oil a large roasting pan.', 81);
INSERT INTO tb_step (name, id_recipe) VALUES ('Place chicken pieces in large bowl. Season with salt, oregano, pepper, rosemary, and cayenne pepper. Add fresh lemon juice, olive oil, and garlic. Place potatoes in bowl with the chicken; stir together until chicken and potatoes are evenly coated with marinade.', 81);
INSERT INTO tb_step (name, id_recipe) VALUES ('Transfer chicken pieces, skin side up, to prepared roasting pan, reserving marinade. Distribute potato pieces among chicken thighs. Drizzle with 2/3 cup chicken broth. Spoon remainder of marinade over chicken and potatoes.', 81);
INSERT INTO tb_step (name, id_recipe) VALUES ('Place in preheated oven. Bake in the preheated oven for 20 minutes. Toss chicken and potatoes, keeping chicken skin side up; continue baking until chicken is browned and cooked through, about 25 minutes more. An instant-read thermometer inserted near the bone should read 165 degrees F (74 degrees C). Transfer chicken to serving platter and keep warm.', 81);
INSERT INTO tb_step (name, id_recipe) VALUES ('Set oven to broil or highest heat setting. Toss potatoes once again in pan juices. Place pan under broiler and broil until potatoes are caramelized, about 3 minutes. Transfer potatoes to serving platter with chicken.', 81);
INSERT INTO tb_step (name, id_recipe) VALUES ('Place roasting pan on stove over medium heat. Add a splash of broth and stir up browned bits from the bottom of the pan. Strain; spoon juices over chicken and potatoes. Top with chopped oregano.', 81);
INSERT INTO tb_step (name, id_recipe) VALUES ('In a medium saucepan combine the stock, chicken, peas, celery and carrots. Bring to a boil.', 82);
INSERT INTO tb_step (name, id_recipe) VALUES ('Mix cornstarch with milk and stir into stock mixture. Cook stirring constantly for 5 minutes. Remove from heat and let cool for 1 hour.', 82);
INSERT INTO tb_step (name, id_recipe) VALUES ('Preheat oven to 325 degrees F (175 degrees C).', 82);
INSERT INTO tb_step (name, id_recipe) VALUES ('Stir cheese into filling mixture and pour into a 9 inch pie crust. Top with second crust, seal edges and cut slits in top crust. Place on a cookie sheet and bake for 35 to 40 minutes or until top crust is golden brown.', 82);
INSERT INTO tb_step (name, id_recipe) VALUES ('Mix mashed potatoes, flour, onion, egg, black pepper, and salt together in a bowl until batter consistency.', 83);
INSERT INTO tb_step (name, id_recipe) VALUES ('Heat vegetable oil in a skillet over medium heat. Drop 4-inch circles of batter into hot oil. Cook until golden brown, about 5 minutes per side. Remove with a slotted spoon and drain on paper towels. Repeat with remaining batter.', 83);
INSERT INTO tb_step (name, id_recipe) VALUES ('Place rib roast on a plate and bring to room temperature, about 4 hours.', 84);
INSERT INTO tb_step (name, id_recipe) VALUES ('Preheat an oven to 500 degrees F (260 degrees C).', 84);
INSERT INTO tb_step (name, id_recipe) VALUES ('Combine butter, pepper, and herbes de Provence in a bowl; mix until well blended. Spread butter mixture evenly over entire roast. Season roast generously with kosher salt.', 84);
INSERT INTO tb_step (name, id_recipe) VALUES ('Roast the 4-pound prime rib (see footnote if using a larger and smaller roast) in the preheated oven for 20 minutes. Turn the oven off and, leaving the roast in the oven with the door closed, let the roast sit in the oven for 2 hours. Remove roast from the oven, slice, and serve.', 84);
INSERT INTO tb_step (name, id_recipe) VALUES ('Preheat oven to 350 degrees F (175 degrees C). Grease a 9x13-inch baking dish.', 85);
INSERT INTO tb_step (name, id_recipe) VALUES ('In a bowl, mix together butter, Dijon mustard, Worcestershire sauce, poppy seeds, and dried onion. Separate the tops from bottoms of the rolls, and place the bottom pieces into the prepared baking dish. Layer about half the ham onto the rolls. Arrange the Swiss cheese over the ham, and top with remaining ham slices in a layer. Place the tops of the rolls onto the sandwiches. Pour the mustard mixture evenly over the rolls.', 85);
INSERT INTO tb_step (name, id_recipe) VALUES ('Bake in the preheated oven until the rolls are lightly browned and the cheese has melted, about 20 minutes. Slice into individual rolls through the ham and cheese layers to serve.', 85);
INSERT INTO tb_step (name, id_recipe) VALUES ('Melt butter in a large skillet over medium-high heat. Stir in garlic, and cook for a few seconds until fragrant. Add scallops, and cook for several minutes on one side, then turn over, and continue cooking until firm and opaque.', 86);
INSERT INTO tb_step (name, id_recipe) VALUES ('Remove scallops to a platter, then whisk salt, pepper, and lemon juice into butter. Pour sauce over scallops to serve.', 86);
INSERT INTO tb_step (name, id_recipe) VALUES ('Melt butter in a large skillet over medium heat. Stir in flour; cook for 1 minute. Whisk in chicken broth; cook and stir until sauce is smooth and thickened, 2 to 3 minutes.', 87);
INSERT INTO tb_step (name, id_recipe) VALUES ('Stir green chiles, olives, salt, oregano, and chipotle chile powder into the sauce. Add chicken breasts and bring to a simmer. Reduce heat to low, cover, and cook until chicken is no longer pink in the center and the juices run clear, about 15 minutes. An instant-read thermometer inserted into the center should read at least 165 degrees F (74 degrees C).', 87);
INSERT INTO tb_step (name, id_recipe) VALUES ('Remove chicken from the skillet, leaving heat on low. Place chicken on a plate; shred into bite-size pieces.', 87);
INSERT INTO tb_step (name, id_recipe) VALUES ('Stir sour cream into the sauce. Return chicken to the skillet; add tortilla pieces. Stir until combined and top with Colby-Jack cheese. Cover and cook until bubbly and melted, 5 to 8 minutes.', 87);
INSERT INTO tb_step (name, id_recipe) VALUES ('Preheat oven to 400 degrees F (200 degrees C). Line a baking sheet with aluminum foil.', 88);
INSERT INTO tb_step (name, id_recipe) VALUES ('Mix cream cheese and Cheddar cheese together in a bowl until evenly blended. Fill each jalapeno half with the cheese mixture. Put halves back together and wrap each stuffed pepper with a slice of bacon. Arrange bacon-wrapped peppers on the prepared baking sheet.', 88);
INSERT INTO tb_step (name, id_recipe) VALUES ('Bake in the preheated oven until bacon is crispy, about 15 minutes.', 88);
INSERT INTO tb_step (name, id_recipe) VALUES ('Shred potatoes into a large bowl filled with cold water. Stir until water is cloudy, drain, and cover potatoes again with fresh cold water. Stir again to dissolve excess starch. Drain potatoes well, pat dry with paper towels, and squeeze out any excess moisture.', 89);
INSERT INTO tb_step (name, id_recipe) VALUES ('Heat clarified butter in a large non-stick pan over medium heat. Sprinkle shredded potatoes into the hot butter and season with salt, black pepper, cayenne pepper, and paprika.', 89);
INSERT INTO tb_step (name, id_recipe) VALUES ('Cook potatoes until a brown crust forms on the bottom, about 5 minutes. Continue to cook and stir until potatoes are browned all over, about 5 more minutes.', 89);
INSERT INTO tb_step (name, id_recipe) VALUES ('In a medium bowl, combine grated carrots and brown sugar. Set aside for 60 minutes, then stir in raisins.', 90);
INSERT INTO tb_step (name, id_recipe) VALUES ('Preheat oven to 350 degrees F (175 degrees C). Grease and flour two 10 inch cake pans.', 90);
INSERT INTO tb_step (name, id_recipe) VALUES ('In a large bowl, beat eggs until light. Gradually beat in the white sugar, oil and vanilla. Stir in the pineapple. Combine the flour, baking soda, salt and cinnamon, stir into the wet mixture until absorbed. Finally stir in the carrot mixture and the walnuts. Pour evenly into the prepared pans.', 90);
INSERT INTO tb_step (name, id_recipe) VALUES ('Bake for 45 to 50 minutes in the preheated oven, until cake tests done with a toothpick. Cool for 10 minutes before removing from pan. When completely cooled, frost with cream cheese frosting.', 90);
INSERT INTO tb_step (name, id_recipe) VALUES ('Cook pasta in a large pot of boiling water until done. Drain.', 91);
INSERT INTO tb_step (name, id_recipe) VALUES ('Meanwhile, heat the oil in a frying pan over medium low heat. Add pesto, onion, and salt and pepper. Cook about five minutes, or until onions are soft.', 91);
INSERT INTO tb_step (name, id_recipe) VALUES ('In a large bowl, mix pesto mixture into pasta. Stir in grated cheese. Serve.', 91);
INSERT INTO tb_step (name, id_recipe) VALUES ('Place the bacon in a large stockpot and cook over medium-high heat until crispy, about 10 minutes. Add the onion and garlic; cook and stir until the onion caramelizes; about 10 minutes. Immediately stir in the cabbage and continue to cook and stir another 10 minutes. Season with salt, pepper, onion powder, garlic powder, and paprika. Reduce heat to low, cover, and simmer, stirring occasionally, about 30 minutes more.', 92);
INSERT INTO tb_step (name, id_recipe) VALUES ('Preheat the oven to 400 degrees F (200 degrees C). Lightly grease the bottom of a deep-dish pie plate.', 93);
INSERT INTO tb_step (name, id_recipe) VALUES ('Beat butter, sugar, and egg in a bowl until creamy. Blend in flour to form a dough, using your hands to knead it together at the end.', 93);
INSERT INTO tb_step (name, id_recipe) VALUES ('Roll pastry out on a lightly floured surface and place into the prepared pie plate so it covers the bottom and the sides.', 93);
INSERT INTO tb_step (name, id_recipe) VALUES ('Beat eggs and sugar in a bowl using an electric mixer. Beat in vanilla extract. Gradually stir in cold milk until fully incorporated. Pour custard mixture over the pastry base and sprinkle with nutmeg.', 93);
INSERT INTO tb_step (name, id_recipe) VALUES ('Bake in the preheated oven until custard is set and a sharp knife inserted near the centre comes out clean, about 45 minutes. Remove from the oven and cool completely before serving, about 1 hour.', 93);
INSERT INTO tb_step (name, id_recipe) VALUES ('Season pork chops with salt, pepper, and garlic salt to taste.', 94);
INSERT INTO tb_step (name, id_recipe) VALUES ('In a large skillet, brown the chops over medium-high heat. Add the onion and mushrooms, and saute for one minute. Pour cream of mushroom soup over chops. Cover skillet, and reduce temperature to medium-low. Simmer 20 to 30 minutes, or until chops are cooked through.', 94);
INSERT INTO tb_step (name, id_recipe) VALUES ('Preheat oven to 450 degrees F (230 degrees C). Move oven rack to the center position.', 95);
INSERT INTO tb_step (name, id_recipe) VALUES ('In a large bowl, combine bread crumbs, garlic, rosemary, 1 teaspoon salt and 1/4 teaspoon pepper. Toss in 2 tablespoons olive oil to moisten mixture. Set aside.', 95);
INSERT INTO tb_step (name, id_recipe) VALUES ('Season the rack all over with salt and pepper. Heat 2 tablespoons olive oil in a large heavy oven proof skillet over high heat. Sear rack of lamb for 1 to 2 minutes on all sides. Set aside for a few minutes. Brush rack of lamb with the mustard. Roll in the bread crumb mixture until evenly coated. Cover the ends of the bones with foil to prevent charring.', 95);
INSERT INTO tb_step (name, id_recipe) VALUES ('Arrange the rack bone side down in the skillet. Roast the lamb in preheated oven for 12 to 18 minutes, depending on the degree of doneness you want. With a meat thermometer, take a reading in the center of the meat after 10 to 12 minutes and remove the meat, or let it cook longer, to your taste. Let it rest for 5 to 7 minutes, loosely covered, before carving between the ribs.', 95);
INSERT INTO tb_step (name, id_recipe) VALUES ('Preheat oven to 400 degrees F (200 degrees C). Lightly grease a baking sheet.', 96);
INSERT INTO tb_step (name, id_recipe) VALUES ('In a large bowl, combine flour, sugar, baking powder, and salt. Cut in butter. Mix the egg and milk in a small bowl, and stir into flour mixture until moistened.', 96);
INSERT INTO tb_step (name, id_recipe) VALUES ('Turn dough out onto a lightly floured surface, and knead briefly. Roll dough out into a 1/2 inch thick round. Cut into 8 wedges, and place on the prepared baking sheet.', 96);
INSERT INTO tb_step (name, id_recipe) VALUES ('Bake 15 minutes in the preheated oven, or until golden brown.', 96);
INSERT INTO tb_step (name, id_recipe) VALUES ('Peel and mash avocados in a medium serving bowl. Stir in onion, garlic, tomato, lime juice, salt and pepper. Season with remaining lime juice and salt and pepper to taste. Chill for half an hour to blend flavors.', 97);
INSERT INTO tb_step (name, id_recipe) VALUES ('Preheat oven to 325 degrees F (165 degrees C). Spray a 9x13-inch glass baking dish with cooking spray.', 98);
INSERT INTO tb_step (name, id_recipe) VALUES ('Unroll crescent rolls into the prepared dish, pinching the seams together.', 98);
INSERT INTO tb_step (name, id_recipe) VALUES ('Beat cream cheese, white sugar, and vanilla extract together in a bowl; spread over crescent roll dough. Sprinkle brown sugar and 1 teaspoon cinnamon over cream cheese mixture layer.', 98);
INSERT INTO tb_step (name, id_recipe) VALUES ('Pour cherry pie filling over brown sugar layer. Sprinkle yellow cake mix over cherry pie filling. Arrange butter slices over cake mix and top with remaining 1 teaspoon cinnamon. Wrap a piece of aluminum foil under baking dish to help prevent rolls from burning.', 98);
INSERT INTO tb_step (name, id_recipe) VALUES ('Bake in the preheated oven until cooked through, about 1 hour. Cool for 10 to 15 minutes before serving.', 98);
INSERT INTO tb_step (name, id_recipe) VALUES ('Whisk together the flour, milk, water, baking powder, and salt in a bowl until smooth.', 99);
INSERT INTO tb_step (name, id_recipe) VALUES ('Preheat the oven to 350 degrees F (175 degrees C).', 100);
INSERT INTO tb_step (name, id_recipe) VALUES ('Mix cream cheese with 2 cups salsa and 2 chopped green onions in a bowl until thoroughly combined; lightly stir 2 cups shredded Cheddar and 2 cups Monterey Jack cheese into cream cheese mixture. Spoon a scant 1/2 cup of cheese filling in a line down the center of each tortilla and roll tortilla into a log around the filling. Repeat with remaining tortillas and cheese filling. Place rolled tortillas into a 9x13-inch baking dish.', 100);
INSERT INTO tb_step (name, id_recipe) VALUES ('Spread top of enchiladas with remaining 1 cup salsa. Lightly mix remaining 1/2 cup Cheddar cheese and 1/2 cup Monterey Jack cheese in a bowl and spread over the salsa. Sprinkle with remaining green onions. Cover dish with aluminum foil.', 100);
INSERT INTO tb_step (name, id_recipe) VALUES ('Bake in preheated oven until cheese is melted and bubbly, about 20 minutes.', 100);

